{
  "buildCommand": "rm -rf node_modules .pnpm-store && pnpm install --force && pnpm run build",
  "installCommand": "pnpm install --force",
  "framework": "nextjs",
  "env": {
    "CI": "true"
  },
  "build": {
    "env": {
      "CI": "true",
      "NEXT_TELEMETRY_DISABLED": "1",
      "SKIP_ENV_VALIDATION": "true"
    }
  }
}